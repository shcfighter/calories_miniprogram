view{
    position: relative;
    font-family: "微软雅黑";
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}


/*swiper轮播点*/
.dots{
    position: absolute;
    width:100%;
    height:20rpx;
    bottom: 20rpx;  
    display: flex;  
    display: -webkit-flex;  
  justify-content: center; 
  -webkit-justify-content: center;  
}
.dots view{
   margin:0 10rpx;
   width:20rpx;
   height:20rpx;
   border-radius:50%;
   background: rgba(255,255,255,0.3); 
}
.dots .dotactive{
   background: rgba(255,255,255,0.8);
}
.vip-dots{
    position: absolute;
    width:100%;
    height:20rpx;
    bottom: 20rpx;  
    display: flex;  
    display: -webkit-flex;  
  justify-content: center; 
  -webkit-justify-content: center;  
}
.vip-dots view{
   margin:0 10rpx;
   width:20rpx;
   height:20rpx;
   border-radius:50%;
   background: rgba(255,255,255,0.3); 
}
.vip-dots .dotactive{
   background: rgba(255,255,255,0.8);
}



